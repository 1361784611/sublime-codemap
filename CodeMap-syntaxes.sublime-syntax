%YAML 1.2
---
# http://www.sublimetext.com/docs/3/syntax.html
name: CodeMap Syntax Autodetection
scope: text.codemap
contexts:
  main:
    - match: ^/[{.*
      scope: text.codemap.json

    - match: '\s*?(class|def)[^=]*$'
      scope: text.codemap.python

    - match: '\s*?(public|private)[^=]*$'
      scope: text.codemap.c
